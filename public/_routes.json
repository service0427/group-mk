{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [
    "/build/*",
    "/assets/*",
    "/media/*",
    "/*.js",
    "/*.css",
    "/*.map",
    "/*.json",
    "/*.ico",
    "/*.png",
    "/*.jpg",
    "/*.jpeg",
    "/*.svg",
    "/*.gif",
    "/*.woff",
    "/*.woff2",
    "/*.ttf",
    "/*.eot"
  ],
  "routes": [
    {
      "src": "/advertise/ntraffic/desc",
      "dest": "/index.html"
    },
    {
      "src": "/advertise/ntraffic/campaign",
      "dest": "/index.html"
    },
    {
      "src": "/admin/campaigns/ntraffic",
      "dest": "/index.html"
    },
    {
      "src": "/auth/*",
      "dest": "/index.html"
    },
    {
      "src": "/error/*",
      "dest": "/index.html"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}